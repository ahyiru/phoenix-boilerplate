    "@types/react-dom": "^15.5.0",
    "@types/react-router": "^4.0.11",
    "babel-polyfill": "^6.23.0",
    "echarts": "^3.6.1",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.12.0",
    "lodash": "^4.17.4",
    "mobx": "^3.1.11",
    "mobx-react": "^4.2.1",
