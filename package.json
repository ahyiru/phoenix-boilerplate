{
  "name": "phoenix-boilerplate",
  "version": "0.0.1",
  "main": "scripts/start.js",
  "description": "phoenix-boilerplate",
  "scripts": {
    "start": "nodemon --watch scripts/start.js",
    "build": "webpack --config scripts/webpack.production.js --progress && npm run clean && mv _dist build",
    "clean": "rimraf build",
    "test": "npm run lint && jest",
    "lint": "eslint 'app/**/*.{js,jsx}'",
    "precommit": "npm run test"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "modulePathIgnorePatterns": [
      "/build/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "node"
    ],
    "transform": {
      "\\.tsx?$": "./node_modules/typescript-babel-jest",
      "\\.jsx?$": "./node_modules/babel-jest"
    },
    "collectCoverage": true,
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.(ts|tsx|js|jsx)$"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahyiru/phoenix-boilerplate"
  },
  "license": "MIT",
  "devDependencies": {
    "@ersinfotech/merge": "^1.0.0",
    "@types/jest": "^19.2.4",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "babel-runtime": "^6.22.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cors": "^2.8.2",
    "cron": "^1.2.1",
    "css-loader": "^0.28.4",
    "del": "^2.2.2",
    "enzyme": "^2.8.0",
    "es6-promise": "^4.1.0",
    "eslint": "^3.18.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-react": "^7.0.1",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "gulp4": "^4.0.1",
    "gulp-imagemin": "^3.3.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.4",
    "imagemin-pngquant": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^20.0.4",
    "jwt-simple": "^0.5.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "log4js": "^1.1.1",
    "marked": "^0.3.6",
    "mobx-react-devtools": "^4.2.13",
    "mongoose": "^4.10.4",
    "morgan": "^1.8.2",
    "nodemailer": "^4.0.1",
    "opn": "^5.0.0",
    "passport": "^0.3.2",
    "react-addons-test-utils": "^15.4.2",
